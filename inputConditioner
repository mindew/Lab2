#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-107-gab6ae79)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x263fc80 .scope module, "testConditioner" "testConditioner" 2 6;
 .timescale 0 0;
v0x265c8a0_0 .var "clk", 0 0;
v0x265c960_0 .net "conditioned", 0 0, v0x265c240_0;  1 drivers
v0x265ca30_0 .net "falling", 0 0, v0x265c3f0_0;  1 drivers
v0x265cb30_0 .var "noisysignal", 0 0;
v0x265cc00_0 .net "rising", 0 0, v0x265c5c0_0;  1 drivers
E_0x2640670 .event negedge, v0x265c240_0;
E_0x2640260 .event posedge, v0x265c240_0;
S_0x263fe00 .scope module, "inputconditionerDUT" "inputconditioner" 2 15, 3 8 0, S_0x263fc80;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "noisysignal"
    .port_info 2 /OUTPUT 1 "conditioned"
    .port_info 3 /OUTPUT 1 "positiveedge"
    .port_info 4 /OUTPUT 1 "negativeedge"
P_0x263c2a0 .param/l "counterwidth" 0 3 17, +C4<00000000000000000000000000000011>;
P_0x263c2e0 .param/l "waittime" 0 3 18, +C4<00000000000000000000000000000011>;
v0x2606cf0_0 .net "clk", 0 0, v0x265c8a0_0;  1 drivers
v0x265c240_0 .var "conditioned", 0 0;
v0x265c300_0 .var "counter", 2 0;
v0x265c3f0_0 .var "negativeedge", 0 0;
v0x265c4b0_0 .net "noisysignal", 0 0, v0x265cb30_0;  1 drivers
v0x265c5c0_0 .var "positiveedge", 0 0;
v0x265c680_0 .var "synchronizer0", 0 0;
v0x265c740_0 .var "synchronizer1", 0 0;
E_0x2640500 .event posedge, v0x2606cf0_0;
    .scope S_0x263fe00;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x265c300_0, 0, 3;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265c680_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265c740_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x263fe00;
T_1 ;
    %wait E_0x2640500;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x265c5c0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x265c3f0_0, 0;
    %load/vec4 v0x265c240_0;
    %load/vec4 v0x265c740_0;
    %cmp/e;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x265c300_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x265c300_0;
    %pad/u 32;
    %cmpi/e 3, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x265c300_0, 0;
    %load/vec4 v0x265c240_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x265c740_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x265c3f0_0, 0;
T_1.4 ;
    %load/vec4 v0x265c240_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x265c740_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x265c5c0_0, 0;
T_1.6 ;
    %load/vec4 v0x265c740_0;
    %assign/vec4 v0x265c240_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x265c300_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x265c300_0, 0;
T_1.3 ;
T_1.1 ;
    %load/vec4 v0x265c4b0_0;
    %assign/vec4 v0x265c680_0, 0;
    %load/vec4 v0x265c680_0;
    %assign/vec4 v0x265c740_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x263fc80;
T_2 ;
    %vpi_call 2 29 "$dumpfile", "inputconditioner.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265c8a0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x263fc80;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265c8a0_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x263fc80;
T_4 ;
    %delay 10, 0;
    %load/vec4 v0x265c8a0_0;
    %nor/r;
    %store/vec4 v0x265c8a0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x263fc80;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 20, 0;
    %load/vec4 v0x265c960_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %jmp/0xz  T_5.0, 4;
    %vpi_call 2 48 "$display", "Conditioned Test case 1 Failed" {0 0 0};
T_5.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 20, 0;
    %load/vec4 v0x265c960_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_5.2, 4;
    %vpi_call 2 56 "$display", "Conditioned Test case 2 Failed" {0 0 0};
T_5.2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x265c680_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_5.4, 4;
    %vpi_call 2 64 "$display", "Sync Test case 3 Failed" {0 0 0};
T_5.4 ;
    %delay 15, 0;
    %load/vec4 v0x265c740_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_5.6, 4;
    %vpi_call 2 70 "$display", "Sync Test case 4 Failed" {0 0 0};
T_5.6 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 300, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x265cb30_0, 0, 1;
    %delay 250, 0;
    %load/vec4 v0x265c960_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %jmp/0xz  T_5.8, 4;
    %vpi_call 2 83 "$display", "Debouncing to Rest High Test Case 5 failed." {0 0 0};
T_5.8 ;
    %delay 5, 0;
    %vpi_call 2 88 "$finish" {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x263fc80;
T_6 ;
    %wait E_0x2640260;
    %delay 5, 0;
    %load/vec4 v0x265cc00_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %inv;
    %pushi/vec4 100, 0, 64;
    %vpi_func 2 94 "$time" 64 {0 0 0};
    %cmp/u;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.0, 8;
    %vpi_call 2 95 "$display", "Positive Edge Test Case 6 failed" {0 0 0};
T_6.0 ;
    %jmp T_6;
    .thread T_6;
    .scope S_0x263fc80;
T_7 ;
    %wait E_0x2640670;
    %delay 5, 0;
    %load/vec4 v0x265ca30_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %inv;
    %pushi/vec4 100, 0, 64;
    %vpi_func 2 102 "$time" 64 {0 0 0};
    %cmp/u;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_7.0, 8;
    %vpi_call 2 103 "$display", "Negative Edge Test Case 7 failed" {0 0 0};
T_7.0 ;
    %jmp T_7;
    .thread T_7;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "inputconditioner.t.v";
    "./inputconditioner.v";
