    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x21e1150_0, 0;
T_1.6 ;
    %load/vec4 v0x21e12d0_0;
    %assign/vec4 v0x21e0dd0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x21e0e90_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x21e0e90_0, 0;
T_1.3 ;
T_1.1 ;
    %load/vec4 v0x21e1040_0;
    %assign/vec4 v0x21e1210_0, 0;
    %load/vec4 v0x21e1210_0;
    %assign/vec4 v0x21e12d0_0, 0;
